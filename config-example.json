{
  "minter_api_url": "http://127.0.0.1:8841/",
  "minter_nodes_pub_keys": [
      "",""
  ],

  "telegram_bot_token": "",
  "monitoring_auth_key": "",
  "telegram_chats_file": "/home/minter-monitoring/chats.pickle",

  "logger": {
    "version": 1,
    "formatters": {
      "precise": {
        "format": "%(asctime)s %(name)-15s %(levelname)-8s %(message)s"
      }
    },
    "handlers": {
      "file": {
        "class": "logging.handlers.RotatingFileHandler",
        "level":"INFO",
        "formatter": "precise",
        "filename": "/home/minter-monitoring/minter-monitoring.log"
       }
    },
    "root": {
      "level": "INFO",
      "handlers": ["file"]
    }
  }
}